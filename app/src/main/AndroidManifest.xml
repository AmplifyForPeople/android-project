<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="amplify.us.amplify">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".WelcomeActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.FullScreenTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/AppTheme.FullScreenTheme" />
        <activity
            android:name=".DescriptionActivity"
            android:label="@string/title_description"
            android:theme="@style/AppTheme.FullScreenTheme" />
        <activity
            android:name=".LoginActivity"
            android:label="@string/title_activity_login"
            android:theme="@style/AppTheme.FullScreenTheme" />
        <activity
            android:name=".RegisterActivity"
            android:label="@string/register"
            android:theme="@style/AppTheme.FullScreenTheme" />
        <activity
            android:name=".SetSelfDataActivity"
            android:label="SetSelfData"
            android:theme="@style/AppTheme.FullScreenTheme" />
        <activity
            android:name=".SetGenresActivity"
            android:label="SetGenres"
            android:theme="@style/AppTheme.FullScreenTheme" />
        <activity
            android:name=".DetailEstablishmentActivity"
            android:theme="@style/AppTheme.FullScreenTheme" />
        <activity
            android:name=".DetailSongActivity"
            android:theme="@style/AppTheme.FullScreenTheme" />
        <activity
            android:name=".FavouriteSongListActivity"
            android:theme="@style/AppTheme.FullScreenTheme" />
        <activity
            android:name=".EditProfileActivity"
            android:theme="@style/AppTheme.FullScreenTheme" />
        <activity
            android:name=".ChangePasswordActivity"
            android:theme="@style/AppTheme.FullScreenTheme">
        </activity>
    </application>

</manifest>